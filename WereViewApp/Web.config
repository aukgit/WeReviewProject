    <!--<add name="DevEntities" connectionString="Data Source=(LocalDb)\v11.0;AttachDbFilename=|DataDirectory|\config-maganize.mdf;Initial Catalog=config-maganize;Integrated Security=True" providerName="System.Data.SqlClient" />-->
    <add name="WereViewAppEntities" connectionString="metadata=res://*/Models.EntityModel.WereViewAppModel.csdl|res://*/Models.EntityModel.WereViewAppModel.ssdl|res://*/Models.EntityModel.WereViewAppModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=(LocalDB)\v11.0;attachdbfilename=|DataDirectory|\WereViewApp.mdf;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>

  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="elmah.mvc.disableHandler" value="false" />
    <add key="elmah.mvc.disableHandleErrorFilter" value="false" />
    <add key="elmah.mvc.requiresAuthentication" value="false" />
    <add key="elmah.mvc.IgnoreDefaultRoute" value="true" />
    <add key="elmah.mvc.allowedRoles" value="admin" />
    <add key="elmah.mvc.allowedUsers" value="?" />
    <add key="elmah.mvc.route" value="Admin/elmah" />
    <add key="TimeZoneId" value="Bangladesh Standard Time" />
  </appSettings>

  <system.web>
    <customErrors mode="RemoteOnly" />
    <!-- cookie free domain -->
    <sessionState cookieless="UseDeviceProfile" regenerateExpiredSessionId="true" timeout="50" />
    <!--<sessionState cookieless="UseUri"/>-->
    <caching>
      <outputCache enableOutputCache="false" />
      <outputCacheSettings>
        <outputCacheProfiles>
          <add name="YearNoParam" duration="31104000" varyByParam="none" varyByCustom="none" location="Server" />
          <add name="Year" duration="31104000" varyByParam="none" varyByCustom="byuser" location="Server" />
          <add name="Month" duration="2592000" varyByParam="none" varyByCustom="byuser" location="Server" />
          <add name="Day" duration="86400" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="Hour" duration="3600" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="TenMins" duration="600" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="Ten" duration="600" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="Long" duration="300" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="Five" duration="300" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="FiveMins" duration="300" varyByParam="byuser" varyByCustom="byuser" location="Server" />
          <add name="Medium" duration="150" varyByCustom="byuser" location="Server" />
          <add name="TwoMins" duration="120" varyByCustom="byuser" location="Server" />
          <add name="OneMin" duration="60" varyByCustom="byuser" location="Server" />
          <add name="Short" duration="20" varyByCustom="byuser" location="Server" />
          <add name="VeryShort" duration="5" varyByCustom="byuser" location="Server" />
          <add name="ThreeSec" duration="3" varyByCustom="byuser" location="Server" />
        </outputCacheProfiles>
      </outputCacheSettings>
    </caching>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" maxRequestLength="10048576" enableVersionHeader="false" />
    <httpModules>
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" />
    </httpModules>
    <globalization culture="en-US" uiCulture="en-US" enableClientBasedCulture="false" requestEncoding="utf-8" responseEncoding="utf-8" />
  </system.web>

  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
      <!--<mimeMap fileExtension=".woff" mimeType="application/font-woff"/>-->

      <clientCache cacheControlMode="UseExpires" httpExpires="Tue, 13 Apr 2100 14:59:04 GMT" cacheControlCustom="public" />
    </staticContent>
    <urlCompression doStaticCompression="true" doDynamicCompression="true" />
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="1073741824" />
      </requestFiltering>
    </security>
    <modules runAllManagedModulesForAllRequests="true">
      <remove name="FormsAuthenticationModule" />
      <!--<add name="HeaderRemove" type="HttpModules.HeaderRemove" />-->
      <add name="ErrorLog" type="Elmah.ErrorLogModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorMail" type="Elmah.ErrorMailModule, Elmah" preCondition="managedHandler" />
      <add name="ErrorFilter" type="Elmah.ErrorFilterModule, Elmah" preCondition="managedHandler" />
      <add name="RemoveETag" type="WereViewApp.Modules.Reponse.RemoveETagModule" />
    </modules>
    <validation validateIntegratedModeConfiguration="false" />
    
    <httpErrors errorMode="Detailed" />
    <asp scriptErrorSentToBrowser="true" />
    <httpProtocol>
      <customHeaders>
        <clear />
        <remove name="X-Powered-By" />
        <remove name="X-SourceFiles" />
        <!--<remove name="Cookie" />-->
      </customHeaders>
    </httpProtocol>
  <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers></system.webServer>

