@model List<WereViewApp.Models.POCO.Identity.ApplicationUser>
<h2 class="h2" title="Developers Profile">Developers Profile</h2>
<div class="row app-date-seperation seo-optimize">
    <div class="col-md-10"></div>
    <div class="col-md-2  text-right">
        <a>
            <h1 class="month-seperation regular-color bold-important"></h1>
        </a>
    </div>
</div>
<div class="row">
    <div class="archive-app-collection-wrapper">
        <ul class="seo-optimize regular-color archive-app-collection-list search-page-apps-list pagination-required">
            @foreach (var user in Model) {
                <li class="slim-app-view">
                    <a class="app-url" href="@AppVar.Url/@user.UserName" title="user apps:  @user.FirstName @user.LastName">
                        <div class="app-icon">
                            <img src="@app.GallerySearchIconLocation" alt="app"/>
                        </div>
                        <div class="app-content">
                            <h1 class="app-title" title="user apps:  @user.FirstName @user.LastName">
                                @user.FirstName @user.LastName
                                <span class="paragraph-end"></span>
                            </h1>
                            <h3 title="user name: @user.UserName">
                                @user.UserName
                                <span class="paragraph-end"></span>
                            </h3>
                        </div>
                    </a>
                </li>
            }

        </ul>
        <div class="clear"></div>
        <div class="pagination-wrapper text-center">
            <ul class="pagination">
                @ViewBag.paginationHtml
            </ul>
        </div>
    </div>

</div>