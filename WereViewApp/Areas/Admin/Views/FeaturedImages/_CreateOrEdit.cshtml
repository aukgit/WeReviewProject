@model WereViewApp.Models.EntityModel.POCO.FeaturedImage

<div class="form-group AppID-main">
    <div class="controls">
        <label class="col-md-2 control-label">@Html.DisplayNameFor(m => m.AppID)<span class="red ">*</span></label>
        <div class="col-md-10 AppID-combo-div">
            @Html.DropDownList("AppID", null, htmlAttributes: new { @class = "form-control selectpicker", data_live_search = true })
            <a href="#" data-toggle="tooltip" data-original-title="Selected app will be avilable in the homepage gallery." title="Selected app will be avilable in the homepage gallery." class="tooltip-show"><span class="glyphicon glyphicon-question-sign font-size-22 glyphicon-top-fix almost-white"></span></a>
        </div>
    </div>
</div>

@Html.EditorFor(model => model.IsFeatured)

